{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "common/*": ["../common/*"]
      //"*": ["node_modules/*", "src/types/*"]
    }
  },
  "include": ["src/**/*", "interfaces"]
}
